{
  "name": "@intel-enterprise-rag-ui/docsum",
  "private": true,
  "version": "2.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,jsx,ts,tsx,css,md,json,css,scss} --config ./.prettierrc.json",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@intel-enterprise-rag-ui/auth": "workspace:^",
    "@intel-enterprise-rag-ui/components": "workspace:^",
    "@intel-enterprise-rag-ui/control-plane": "workspace:^",
    "@intel-enterprise-rag-ui/icons": "workspace:^",
    "@intel-enterprise-rag-ui/input-validation": "workspace:^",
    "@intel-enterprise-rag-ui/layouts": "workspace:^",
    "@intel-enterprise-rag-ui/markdown": "workspace:^",
    "@intel-enterprise-rag-ui/tailwind-theme": "workspace:^",
    "@intel-enterprise-rag-ui/utils": "workspace:^",
    "docx": "^9.5.1"
  }
}
