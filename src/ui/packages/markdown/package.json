{
  "name": "@intel-enterprise-rag-ui/markdown",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "license": "Apache-2.0",
  "description": "Markdown renderer for Intel Enterprise RAG UI",
  "main": "./dist/intel-enterprise-rag-ui-markdown.umd.cjs",
  "module": "./dist/intel-enterprise-rag-ui-markdown.js",
